{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flask Debug",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "args": ["--app", "app.py", "run", "--debug"],
      "env": {
        "FLASK_APP": "app.py"
      },
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
